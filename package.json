{
  "name": "nf-wordpress",
  "version": "0.0.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/nonfiction/nf-wordpress.git"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "android 4",
    "opera 12",
    "not ie < 11"
  ],
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6
    },
    "plugins": [
      "compat"
    ],
    "rules": {
      "compat/compat": "warn"
    }
  },
  "stylelint": {
    "plugins": [
      "stylelint-no-unsupported-browser-features"
    ],
    "rules": {
      "plugin/no-unsupported-browser-features": true
    },
    "defaultSeverity": "warning"
  },
  "scripts": {
    "build": "webpack --progress --config webpack.config.js",
    "build:production": "webpack --env.production --progress --config webpack.config.js",
    "build:profile": "webpack --progress --profile --json --config webpack.config.js",
    "start": "webpack --hide-modules --watch --config webpack.config.js",
    "dev": "webpack-dev-server",
    "lint": "npm run -s lint:scripts && npm run -s lint:styles",
    "lint:scripts": "eslint web/app/nf/theme/scripts web/app/nf/theme/scripts/dist",
    "lint:styles": "stylelint \"web/app/nf/theme/styles/**/*.{css,sass,scss,sss,less}\"",
    "test": "npm run -s lint"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "assets-webpack-plugin": "^3.9.10",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-compat": "^3.5.1",
    "file-loader": "^5.0.2",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "jquery": "^3.4.1",
    "mini-css-extract-plugin": "^0.9.0",
    "normalize.css": "^8.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss": "^7.0.26",
    "postcss-cli": "^7.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^8.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-safe-parser": "^4.0.1",
    "react": "^16.12.0",
    "style-loader": "^1.1.3",
    "stylelint": "^13.0.0",
    "stylelint-no-unsupported-browser-features": "^4.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.2"
  }
}
