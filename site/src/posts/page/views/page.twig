{% set template = 'page' %} 
{% extends "base.twig" %}



{% block content %}

<!-- Main Content -->
<section id="content" role="main" class="content">
  <div class="content__container content__container--with-sidebar">

    <article class="main post-type-{{post.post_type}}" id="post-{{post.ID}}">
      {{post.content}}
    </article>

  </div>
</section>

{% endblock %}
